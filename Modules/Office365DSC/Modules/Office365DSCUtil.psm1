function Get-TeamByGroupID
{
    [CmdletBinding()]
    [OutputType([Boolean])]
    param
    (
        [Parameter(Mandatory = $true)]
        [System.String]
        $GroupId
    )

    $team = Get-Team  |  Where-Object {($_.GroupId -eq $GroupId)}
    if ($null -eq $team)
    {
        return $false
    }
    return $true
}
