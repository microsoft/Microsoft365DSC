        "read": [
            "Attribute Definition Reader"
        ],
        "update": [
            "Attribute Definition Administrator"
        ]
    },
    "permissions": {
        "graph": {
            "delegated": {
                "read": [],
                "update": []
            },
            "application": {
                "read": [
                    {
                        "name": "CustomSecAttributeDefinition.Read.All"
                    }
                ],
                "update": [
                    {
                        "name": "CustomSecAttributeDefinition.ReadWrite.All"
                    }
                ]
            }
        }
    }
}
